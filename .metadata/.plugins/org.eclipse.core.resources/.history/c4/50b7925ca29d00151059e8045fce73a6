$(document).ready(function(){
	 $.ajax({    
	        type:'get',        
	        url:'./user/getuser',    
	        data:{},    
	        cache:false,    
	        dataType:'json',    
	        success:function(data){  
	        	console.log(data);
	        	alert("Welcome: " + data.userName);
	        }    
	    });    
});