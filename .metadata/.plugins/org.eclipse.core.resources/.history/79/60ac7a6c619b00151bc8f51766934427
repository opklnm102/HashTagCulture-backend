<mapper namespace="listdao">
        <!-- 
               select list by category startdate enddate
        -->
        
         <resultMap type="spring.hashcul.model.ListVo" id="spring.hashcul.model.List">
               <id column="categories" property="categories"/>
               <id column="locations" property="locations"/>
               <id column="startdate" property="categories"/>
                 
               <result column="title" property="title"/>
               <result column="category" property="category"/>
               <result column="startdate" property="startdate"/>
               <result column="enddate" property="enddate"/>
        </resultMap>
        
        
        <select id="getList" resultType="spring.hashcul.model.ListVo">
               select *
               from list_table
               where categories = #{email}
        </select>
</mapper>